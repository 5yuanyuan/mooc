{"remainingRequest":"E:\\study\\junior\\shop\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!E:\\study\\junior\\shop\\src\\components\\ProductList.vue?vue&type=script&lang=js&","dependencies":[{"path":"E:\\study\\junior\\shop\\src\\components\\ProductList.vue","mtime":1623921019874},{"path":"E:\\study\\junior\\shop\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"E:\\study\\junior\\shop\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"E:\\study\\junior\\shop\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"E:\\study\\junior\\shop\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ProductList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ProductList.vue","sourceRoot":"src/components","sourcesContent":["<template>\n<div class=\"container-fluid\">\n  <div class=\"row\">\n    <div class=\"col-lg-12\">\n      <div class=\"card\">\n          <div class=\"card-toolbar clearfix\">\n            <form class=\"pull-right search-bar\" method=\"get\" action=\"#!\" role=\"form\">\n              <div class=\"input-group\">\n                <div class=\"input-group-btn\">\n                  <input type=\"hidden\" name=\"search_field\" id=\"search-field\" value=\"title\">\n                  <button class=\"btn btn-default dropdown-toggle\" id=\"search-btn\" data-toggle=\"dropdown\" type=\"button\" aria-haspopup=\"true\" aria-expanded=\"false\">\n                  标题 <span class=\"caret\"></span>\n                  </button>\n                  <ul class=\"dropdown-menu\">\n                    <li> <a tabindex=\"-1\" href=\"javascript:void(0)\" data-field=\"title\">标题</a> </li>\n                    <li> <a tabindex=\"-1\" href=\"javascript:void(0)\" data-field=\"cat_name\">栏目</a> </li>\n                  </ul>\n                </div>\n                <input type=\"text\" class=\"form-control\" name=\"keyword\" placeholder=\"请输入名称\">\n              </div>\n            </form>\n            <div class=\"toolbar-btn-action\">\n              <a class=\"btn btn-primary m-r-5\" @click=\"goAdd\"><i class=\"mdi mdi-plus\"></i> 新增</a>\n            </div>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"table-responsive\">\n              <table class=\"table table-bordered\">\n                <thead>\n                  <tr>\n                    <th>标题</th>\n                    <th>封面图片</th>\n                    <th>详情图片</th>\n                    <th>排序</th>\n                    <th>操作</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"item in productList\" :key=\"item.id\">\n                    <td>{{ item.title }}</td>\n                    <td><img :src=\"`${item.coverImage}?x-oss-process=image/resize,h_100,m_lfit`\"></td>\n                    <td><img :src=\"`${item.detailImage}?x-oss-process=image/resize,h_100,m_lfit`\"></td>\n                    <td>\n                      <select @change=\"changeOrder\" v-model=\"item.orderNum\">\n                        <option v-for=\"index of 50\" :value=\"index\" :key=\"index\">{{ index }}</option>\n                      </select>\n                    </td>\n                    <td>\n                      <div class=\"btn-group\">\n                        <a class=\"btn btn-xs btn-default\" @click=\"goto(item.id)\" title=\"编辑\" data-toggle=\"tooltip\"><i class=\"mdi mdi-pencil\"></i></a>\n                        <a class=\"btn btn-xs btn-default\" title=\"查看\" data-toggle=\"tooltip\"><i class=\"mdi mdi-eye\"></i></a>\n                        <a class=\"btn btn-xs btn-default\" title=\"删除\" data-toggle=\"tooltip\"><i class=\"mdi mdi-window-close\"></i></a>\n                      </div>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n</div>\n</template>\n\n<script>\n\nimport axios from 'axios'\nimport qs from 'qs'\n\nexport default {\n  name: \"ProductList\",\n  data() {\n    return {\n      productList: []\n    }\n  },\n\n  mounted() {\n    this.loadProducts()\n  },\n  methods: {\n    loadProducts: function () {\n      var that = this\n      let token = localStorage.getItem(\"token\")\n      if (!token) {\n        that.$router.push(\"/login\")\n      } else {\n        // console.log(token)\n        axios.get(that.GLOBAL.API_ROOT + \"/products\", {\n          params: {\n            \"category\": \"product\",\n          },\n          headers:{ \"token\": token }\n        }).then(function (res) {\n          that.productList = res.data\n          // console.log(that.productList)\n        },\n        function (err) {\n          console.log(err)\n        })\n      }\n    },\n\n    goto: function(id) {\n      var that = this\n      that.$router.push({\n        name: \"add_product\",\n        query: {\n          \"id\": id\n        }\n      })\n    },\n\n    goAdd: function () {\n      this.$router.push(\"/add_product\")\n    },\n\n    changeOrder: function (event) {\n      var that = this\n      let productId = event.target.id\n      let orderNum = event.target.value\n      axios.post(that.GLOBAL.API_ROOT + \"/change_product_order\", qs.stringify({\n        \"productId\": productId,\n        \"orderNum\": orderNum\n      })).then(function (res) {\n            console.log(res)\n            that.loadProducts()\n          },\n          function (err) {\n            console.log(err)\n          })\n    }\n  },\n}\n</script>\n"]}]}