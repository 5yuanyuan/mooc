CAAA;AAAA,UAAA,IAAA,EAAA,cAAA;AAAA,UAAA,KAAA,EAAA;AAAA,YAAA,EAAA,EAAA;AAAA;AAAA,SAAA;AACA;AACA,KAjBA;AAkBA,IAAA,aAAA,EAAA,uBAAA,EAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA,CAAA,QAAA,GAAA,6BAAA,GAAA,EAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,GAAA,CAAA;AACA,QAAA,IAAA,CAAA,aAAA;AACA,QAAA,IAAA,CAAA,cAAA;AAEA,OARA,EASA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAXA;AAaA,KAjCA;AAkCA,IAAA,aAlCA,2BAkCA;AACA,UAAA,MAAA,GAAA;AACA;AACA,QAAA,OAAA,EAAA;AAAA,0BAAA,kBAAA;AAAA,mBAAA,YAAA,CAAA,OAAA,CAAA,OAAA;AAAA;AAFA,OAAA;AAIA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,KAAA,MAAA,CAAA,QAAA,GAAA,+BAAA,EAAA,IAAA,CAAA,OAAA,EAAA,MAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,EADA,CAEA;;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA,KAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA,SAFA,MAGA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA,SARA,CASA;AACA;;AACA,OAZA;AAaA;AArDA,GAlBA;AAyEA,EAAA,OAzEA,qBAyEA;AACA,SAAA,cAAA;AACA;AA3EA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"panel-group\" id=\"accordion\">\r\n            <div class=\"panel panel-default\" v-for=\"item in List\" :key=\"item.id\">\r\n                <div class=\"panel-heading\">\r\n                    <h4 class=\"panel-title\">\r\n                        <a data-toggle=\"collapse\" data-parent=\"#accordion\" class=\"col-md-10\"\r\n                           :href=\"`#${item.id}`\">\r\n                            {{item.name}}\r\n                        </a>\r\n                        <div class=\"btn-group \">\r\n<!--                            <a class=\"btn btn-xs btn-primary m-r-5\"><i class=\"mdi mdi-plus\"></i> 新增</a>-->\r\n                            <a class=\"btn btn-xs btn-primary m-r-5\" @click=\"GotoAdd(item.id)\"><i class=\"mdi mdi-pencil\"></i>编辑</a>\r\n                            <a class=\"btn btn-xs btn-primary m-r-5\" @click=\"deleteSubject(item.id)\"><i class=\"mdi mdi-window-close\"></i>删除</a>\r\n                        </div>\r\n                    </h4>\r\n                </div>\r\n                <div :id=\"item.id\" class=\"panel-collapse collapse\">\r\n                    <div class=\"panel-body\">\r\n                        <span class=\"label label-outline-primary\" v-for=\"subject in item.children\" :key=\"subject.id\">\r\n                            {{subject.name}}\r\n                            <div class=\"btn-group \">\r\n    <!--                            <a class=\"btn btn-xs btn-primary m-r-5\"><i class=\"mdi mdi-plus\"></i> 新增</a>-->\r\n                                <a class=\"btn btn-xxs btn-cyan m-r-5\" @click=\"GotoAdd(subject.id)\"><i class=\"mdi mdi-pencil\"></i></a>\r\n                                <a class=\"btn btn-xxs btn-cyan m-r-5\" @click=\"deleteSubject(subject.id)\"><i class=\"mdi mdi-window-close\"></i></a>\r\n                            </div>\r\n                        </span>\r\n\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"side-bar\">\r\n            <a @click=\"GotoAdd()\" class=\"icon-qq\"></a>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    import axios from \"axios\";\r\n    export default {\r\n        name: \"SubjectList\",\r\n        data(){\r\n            return{\r\n                List:[],\r\n                subject:{\r\n                    id: \"\",\r\n                    name: \"\",\r\n                    parentCid: \"\",\r\n                    catLevel: null,\r\n                    showStatus: null,\r\n                    sort: null,\r\n                    gmtCreate: 1608304695000,\r\n                    gmtModified: 1608304695000,\r\n                    children: null,\r\n                },\r\n            }\r\n        },\r\n        methods:{\r\n            getSubjectList:function () {\r\n                let config = {\r\n                    headers:{'token':localStorage.getItem(\"token\")}\r\n                };\r\n                var that = this;\r\n                axios.get(that.GLOBAL.API_ROOT + '/api/eduSubject/getSubject/list',config).then(response=>{\r\n                    console.log(response.data);\r\n                    that.List=response.data.data\r\n                })\r\n            },\r\n            GotoAdd:function(id){\r\n                if(id === undefined){\r\n                    this.$router.push({path:\"/add_subject\"})\r\n                }else{\r\n                    this.$router.push({path:\"/add_subject\",query:{id:id}})\r\n                }\r\n            },\r\n            deleteSubject:function (id) {\r\n                var that = this\r\n                axios.get(that.GLOBAL.API_ROOT + \"/api/eduSubject/getSubject/\"+id).then(\r\n                    function (response) {\r\n                        // console.log(response.data.data.subject);\r\n                        that.subject = response.data.data.subject;\r\n                        that.subject.showStatus = 0;\r\n                        that.updateSubject();\r\n                        that.getSubjectList();\r\n\r\n                    },\r\n                    function (err) {\r\n                        console.log(err);\r\n                    }\r\n                )\r\n            },\r\n            updateSubject(){\r\n                let config = {\r\n                    // contentType: “application/json”,\r\n                    headers:{'Content-Type': 'application/json','token':localStorage.getItem(\"token\")}\r\n                };\r\n                let that = this\r\n                axios.put(this.GLOBAL.API_ROOT+\"/api/eduSubject/updateSubject\",that.subject,config)\r\n                    .then(response => {\r\n                        console.log(response)\r\n                        // 提示信息\r\n                        if(response.data.message===\"成功\"){\r\n                            alert(\"修改成功\")\r\n                        }\r\n                        else{\r\n                            alert(\"修改失败\")\r\n                        }\r\n                        // 回到列表页面 路由跳转\r\n                        // this.$router.push({ path: '/teacher_list' })\r\n                    })\r\n            }\r\n        },\r\n        mounted() {\r\n            this.getSubjectList();\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .btn-group-xxs>.btn,\r\n    .btn-xxs {\r\n        font-size: 12px;\r\n        padding: 2px 6px;\r\n        line-height: 10px;\r\n    }\r\n    span{\r\n        border:0\r\n    }\r\n\r\n    .side-bar { border-radius:33px ;width: 66px;position: fixed;bottom: 20px;right: 25px;font-size: 0;line-height: 0;z-index: 100; background-color: #ddd;}\r\n    .side-bar a { border-radius:33px; width: 66px;height: 66px;display: inline-block;margin-bottom: 2px;background: url(\"../../public/static/images2/add.png\");background-size:cover;}\r\n    .side-bar a:hover {background-color: #669fdd;}\r\n    .side-bar .icon-qq {background-position: 0 -62px;}\r\n    .side-bar .icon-chat:hover .chat-tips {display: block;}\r\n    .chat-tips i {width: 9px;height: 16px;display: inline-block;position: absolute;right: -9px;top: 80px;background-position:-88px -350px;}\r\n    .chat-tips img {width: 138px;height: 138px;}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}