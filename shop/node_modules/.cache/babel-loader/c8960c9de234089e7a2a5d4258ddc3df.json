productList\" :key=\"item.id\">\n                    <td>{{ item.title }}</td>\n                    <td><img :src=\"`${item.coverImage}?x-oss-process=image/resize,h_100,m_lfit`\"></td>\n                    <td><img :src=\"`${item.detailImage}?x-oss-process=image/resize,h_100,m_lfit`\"></td>\n                    <td>\n                      <select :id=\"item.id\" @change=\"changeOrder\">\n                        <option v-for=\"index of 50\" :value=\"index\" :key=\"index\">{{ index }}</option>\n                      </select>\n                    </td>\n                    <td>\n                      <div class=\"btn-group\">\n                        <a class=\"btn btn-xs btn-default\" href=\"#!\" title=\"编辑\" data-toggle=\"tooltip\"><i class=\"mdi mdi-pencil\"></i></a>\n                        <a class=\"btn btn-xs btn-default\" href=\"#!\" title=\"查看\" data-toggle=\"tooltip\"><i class=\"mdi mdi-eye\"></i></a>\n                        <a class=\"btn btn-xs btn-default\" href=\"#!\" title=\"删除\" data-toggle=\"tooltip\"><i class=\"mdi mdi-window-close\"></i></a>\n                      </div>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n</div>\n</template>\n\n<script>\n\nimport axios from 'axios'\nimport qs from 'qs'\n\nexport default {\n  name: \"ProductList\",\n  data() {\n    return {\n      productList: []\n    }\n  },\n  mounted() {\n    this.loadProducts()\n  },\n  methods: {\n    loadProducts: function () {\n      console.log('获取产品列表')\n      var that = this\n      axios.get(that.GLOBAL.API_ROOT + \"/products\", {\n        params: {\n          \"category\": \"product\"\n        }\n      }).then(function (res) {\n        that.productList = res.data\n        console.log(that.productList)\n      },\n      function (err) {\n        console.log(err)\n      })\n    },\n    changeOrder: function (event) {\n      var that = this\n      let productId = event.target.id\n      let orderNum = event.target.value\n      axios.post(that.GLOBAL.API_ROOT + \"/change_product_order\", qs.stringify({\n        \"productId\": productId,\n        \"orderNum\": orderNum\n      })).then(function (res) {\n            console.log(res)\n            that.loadProducts()\n          },\n          function (err) {\n            console.log(err)\n          })\n    }\n  },\n}\n</script>\n"],"sourceRoot":"src/components"}]}