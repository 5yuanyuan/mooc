A,EAAA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,QAAA,GAAA,wBAAA,GAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAFA;AAGA,KAnCA;AAqCA,IAAA,KAAA,EAAA,iBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,gBAAA;AACA;AAvCA;AAdA,CAAA","sourcesContent":["<template>\r\n<div class=\"container-fluid\">\r\n  <div class=\"row\">\r\n    <div class=\"col-lg-12\">\r\n      <div class=\"card\">\r\n          <div class=\"card-toolbar clearfix\">\r\n            <form class=\"pull-right search-bar\" method=\"get\" action=\"#!\" role=\"form\">\r\n              <div class=\"input-group\">\r\n                <div class=\"input-group-btn\">\r\n                  <input type=\"hidden\" name=\"search_field\" id=\"search-field\" value=\"title\">\r\n                  <button class=\"btn btn-default dropdown-toggle\" id=\"search-btn\" data-toggle=\"dropdown\" type=\"button\" aria-haspopup=\"true\" aria-expanded=\"false\">\r\n                  标题 <span class=\"caret\"></span>\r\n                  </button>\r\n                  <ul class=\"dropdown-menu\">\r\n                    <li> <a tabindex=\"-1\" href=\"javascript:void(0)\" data-field=\"title\">标题</a> </li>\r\n                    <li> <a tabindex=\"-1\" href=\"javascript:void(0)\" data-field=\"cat_name\">栏目</a> </li>\r\n                  </ul>\r\n                </div>\r\n                <input type=\"text\" class=\"form-control\" name=\"keyword\" placeholder=\"请输入名称\">\r\n              </div>\r\n            </form>\r\n            <div class=\"toolbar-btn-action\">\r\n              <a class=\"btn btn-primary m-r-5\" @click=\"goAdd\"><i class=\"mdi mdi-plus\"></i> 新增</a>\r\n            </div>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table table-bordered\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>id</th>\r\n                    <th>轮播图片</th>\r\n<!--                    <th>详情图片</th>-->\r\n<!--                    <th>排序</th>-->\r\n                    <th>操作</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"item in bannerList\" :key=\"item.id\">\r\n                    <td>{{ item.title }}</td>\r\n                    <td><img :src=\"`${item.imageUrl}?x-oss-process=image/resize,h_100,m_lfit`\"></td>\r\n                    <td>\r\n                      <div class=\"btn-group\">\r\n                        <a class=\"btn btn-xs btn-default\" @click=\"goto(item.id)\" title=\"编辑\" data-toggle=\"tooltip\"><i class=\"mdi mdi-pencil\"></i>编辑</a>\r\n                        <a class=\"btn btn-xs btn-default\" title=\"查看\" data-toggle=\"tooltip\"><i class=\"mdi mdi-eye\"></i>查看</a>\r\n                        <a class=\"btn btn-xs btn-default\" @click=\"delBanner(item.id)\" title=\"删除\" data-toggle=\"tooltip\"><i class=\"mdi mdi-window-close\"></i>删除</a>\r\n                      </div>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\n// import qs from 'qs'\r\n\r\nexport default {\r\n  name: \"ProductList\",\r\n  data() {\r\n    return {\r\n      bannerList: []\r\n    }\r\n  },\r\n\r\n  components: {\r\n  },\r\n\r\n  mounted() {\r\n    this.loadProducts()\r\n  },\r\n  methods: {\r\n    loadProducts: function () {\r\n      var that = this\r\n      let token = localStorage.getItem(\"token\")\r\n      if (!token) {\r\n        that.$router.push(\"/login\")\r\n      } else {\r\n        // console.log(token)\r\n        axios.get(that.GLOBAL.API_ROOT + \"/api/eduBanner\", {\r\n        }).then(function (res) {\r\n          that.bannerList = res.data.data\r\n          console.log(that.bannerList)\r\n        },\r\n        function (err) {\r\n          console.log(err)\r\n        })\r\n      }\r\n    },\r\n\r\n    goto: function(id) {\r\n      var that = this\r\n      that.$router.push({\r\n        name: \"change_banner\",\r\n        query: {\r\n          \"id\": id\r\n        }\r\n      })\r\n    },\r\n\r\n    delBanner: function (id) {\r\n      var that = this\r\n\r\n      axios.delete(that.GLOBAL.API_ROOT + '/api/eduBanner/remove/' + id).then(res => {\r\n        console.log(res)\r\n      })\r\n    },\r\n\r\n    goAdd: function () {\r\n      this.$router.push(\"/change_banner\")\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components"}]}