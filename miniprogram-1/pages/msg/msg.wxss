.msgList {
  width: 100%;
  height: 70px;
}

.text {
  width: 100%;
}

.img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  float: left;
  margin-top: 5px;
  margin-left: 5px;
}

.sender {
  display: inline-block;
  margin-left: 5px;
  font-size: 14px;
}

.title {
  display: inline;
  margin-left: 15px;
  background-color: rgba(57, 185, 57, 0.705);
  border-radius: 5px;
  color: white;
  font-size: 10px;
}

.source {
  display: inline;
  margin-left: 15px;
  background-color: rgba(57, 117, 185, 0.568);
  border-radius: 5px;
  color: white;
  font-size: 10px;
}

.msg {
  width: 270px;
  height: 60px;
  font-size: 14px;
  color: rgba(114, 107, 107, 0.733);
  padding: 5px;
  margin-left: 50px;
}

.no-msg {
  text-align: center;
  margin-top: 50px;
  color: #ddd;
}

.gmtCreate {
  font-size: 10px;
  margin-left: 15px;
  color: rgb(128, 124, 124);
}