.container swiper {
  width: 100%;
  height: 300rpx;
}

.container swiper image {
  width: 100%;
}

.jpList {
  width: auto;
  height: 300px;
}

.mfList {
  width: auto;
  height: 300px;
}

.tac {
  padding: 15rpx;
  text-align: center;
  margin-top: 30rpx;
  color: grey;
}

.img {
  width: 100%;
}

.imgSize {
  width: 100%;
  height: 80px;
  position: relative;
  overflow: hidden
}

.course {
  width: 170px;
  overflow: hidden;
  display: block;
  float: left;
  margin: 0rpx 0rpx 5rpx 15rpx;
}

.title {
  font-size: 30rpx;
  text-align: center;
}

.price {
  font-size: 30rpx;
}

.flowerImage {
  height: 350rpx;
  width: 340rpx;
  border-radius: 10rpx;
  box-shadow: 0rpx 5rpx 10rpx -4rpx rgba(28, 36, 67, 0.45);
}

.oneitem {
  box-shadow: 0rpx 5rpx 10rpx -4rpx rgba(28, 36, 67, 0.45);
  width: 340rpx;
  margin: 10rpx;
  border-radius: 10rpx;
  text-align: center;
  font-size: 30rpx;
}

.row {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: center;
}